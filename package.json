{
  "name": "tabmcp-chatbot-v3",
  "version": "1.0.0",
  "description": "Tableau AI Assistant Chatbot MVP - Natural language interface for Tableau data",
  "type": "module",
  "main": "dist/server/server.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/server/server.ts",
    "build": "tsc && vite build",
    "start": "node dist/server/server.js",
    "type-check": "tsc --noEmit",
    "reload-env": "tsx tools/reload-env.ts",
    "tool-definitions-smoke": "tsx tools/mcp-tool-definitions-smoke.ts --verbose true",
    "tool-definitions:integration": "tsx tools/mcp-tool-definitions-integration.ts --verbose true"
  },
  "keywords": [
    "tableau",
    "ai",
    "chatbot",
    "mcp",
    "analytics"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^9.2.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
